{
  "name": "capacitor-framework7-v8-svelte-tab-view",
  "private": true,
  "version": "1.0.0",
  "description": "capacitor-framework7-v8-svelte-tab-view",
  "repository": "",
  "license": "UNLICENSED",
  "engines": {
    "node": "~18.16.0",
    "npm": "~9.5.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "installfont": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "monaca:preview": "npm run dev & npm run watch",
    "monaca:transpile": "npm run build",
    "monaca:debug": "npm run watch",
    "watch": "cross-env NODE_ENV=production vite build --watch"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/app": "^5.0.3",
    "@capacitor/browser": "^5.0.3",
    "@capacitor/core": "^5.0.5",
    "@capacitor/keyboard": "^5.0.4",
    "@capacitor/splash-screen": "^5.0.4",
    "@capacitor/status-bar": "^5.0.4",
    "@trapezedev/configure": "^7.0.10",
    "dom7": "^4.0.6",
    "framework7": "^8.1.0",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^8.1.0",
    "material-icons": "^1.13.8",
    "skeleton-elements": "^4.0.1",
    "svelte": "^4.2.7",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.0.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^8.4.2",
    "vite": "^5.0.2"
  }
}
